{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Technologies",
  "description": "A list of technologies.",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "type": {
        "type": "string",
        "enum": [
          "Language",
          "Framework",
          "Library",
          "Database",
          "Tool"
        ]
      },
      "logoUrl": {
        "type": "string"
      }
    },
    "required": [
      "id",
      "name",
      "type",
      "logoUrl"
    ]
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Certifications",
  "description": "A list of certifications.",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "issuingOrganization": {
        "type": "string"
      },
      "dateObtained": {
        "type": "string",
        "format": "date"
      },
      "imageUrl": {
        "type": "string"
      },
      "verificationUrl": {
        "type": "string"
      }
    },
    "required": [
      "id",
      "name",
      "issuingOrganization",
      "dateObtained",
      "imageUrl"
    ]
  }
}
